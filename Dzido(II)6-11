Zadania od dok Dzido 6-11
========================
Zad.6.
#include <stdio.h>
int main () {
    int tab[5];
    int i;
    int x=0;
    int w;
    int y;
    for (i=0; i<5; i++) {
        scanf("%i", &tab[i]);
        w=tab[i]+ +x;
        x=w;
        }
    printf("%i ", x);
}    
Zad.7.
#include <stdio.h>
int main () {
    int tab[5];
    int i;
    int x=0;
    int w;
    int y;
    for (i=0; i<5; i++) {
        tab[i]=x;
        scanf("%i", &x);
        if(x%2==0) {
    printf("%i ", x);
        }
        }
}    
Zad.8.
#include <stdio.h>
int main () {
    int tab[5];
    int i;
    int x;
    int y;
    for (i=0; i<6; i++) {
        tab[i]=x;
        scanf("%i", &x);
    }
        if(i=1) {
        printf("%i ", tab[5]);
        }
        for(i=2; i<5; i++) {
        printf("%i ", tab[i]);
        }
        if(i=4) {
        printf("%i ", tab[1]);  
        }
} 
Zad.9.
#include <stdio.h>
int main () {
    int a[5];
    int b[5];
    int i;
    int x;
    for (i=0; i<6; i++) {
        a[i]=x;
        scanf("%i", &x);
        b[i]=x;
        printf("%i ", b[i]);
    }
}
Zad.10.
#include <stdio.h>
int main () {
    int a[5];
    int b[5];
    int i;
    int x;
    for (i=0; i<6; i++) {
        a[i]=x;
        scanf("%i", &x);
        b[i]=2*x;
        printf("%i ", b[i]);
    }
}
Zad.11.
#include <stdio.h>
int main () {
    int a[5];
    int b[10];
    int i;
    int x;
    int y;
    for (i=0; i<6; i++) {
        a[i]=x;
        scanf("%i", &x);
        b[i]=x;
        printf("%i ", b[i]);
        printf("%i ", b[i]);
    }
}
