Program wczytuje kolejne wyrazy ciagu Fibonacchiego
==================================
#include <stdio.h>
int main () {
int n=0;
int m=1;
int b;
int i;
int tab[i];
    scanf("%i", &b);
    int c=b;
    //b? 2
    //b? 0
    if (b>0) {
        printf("0 ");
    }
    if (b>1) {
        printf("1 ");
    }
    b=b-2;
    //b? 3
    for(i=0; i<b; i++) {
        int a=m;
        m=m+n;
        n=a;
        tab[i] =m;
        printf("%i ", m);
    }
    //while(i>0){
      //  i--;
        //printf("%i|%i\n", i, tab[i]);
    //}
//b 5
//
printf("\n");
 
b--;
    while (b>=0) {
      printf("%i ", tab[b]);
      b--;
    } 
    //
    if (c>1) {
        printf("1 ");
    }
    if (c>0) {
        printf("0 ");
    }
}
