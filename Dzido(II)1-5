Zadania od dok Dzido 1-5
============================
Zad.1.
#include <stdio.h>
int main () {
    int tab[]={1,3,7,8,9};
    int i=0;
    while (i<5) {
    printf("%i ", tab[i]);
    i++;
    }
}
 
Zad.2.
#include <stdio.h>
int main () {
    int tab[]={1,3,7,8,9};
    int i=5;
    while (i>=0) {
    printf("%i ", tab[i]);
    i--;
    }
}
 Zad.3.
#include <stdio.h>
int main () {
    int w;
    int k=2;
    int i;
    int tab[5];    
    for (i=0; i<5; i++) {
        w=k*2;
        tab[i]=w;
        printf("%i ", tab[i]);
        k=w;
    }
        
}
Zad.4.
#include <stdio.h>
#include <string.h>
int main () {
    int i;
    char tab[5];
    for (i=1; i<=5; i++) {
       scanf("%c", &tab[i]);
       tab[i];
       printf("%c ", tab[i]);
        }        
 }
Zad.5.
#include <stdio.h>
#include <string.h>
int main () {
    int i;
    char tab[5];
    for (i=5; i>0; i--) {
       scanf("%c", &tab[i]);
       tab[i];
       
       }
       int j=i;
       while (j<=5)  {
       printf("%c ", tab[j]);
       j++;
       }
       
 }
